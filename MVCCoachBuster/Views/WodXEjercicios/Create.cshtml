@model MVCCoachBuster.Models.WodXEjercicio

@{
    ViewData["Title"] = "Crear WodxEjercicio";
}

<h4>@ViewData["Title"]</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdWod" class="control-label"></label>
                <select asp-for="IdWod" class ="form-control" asp-items="ViewBag.WodId"></select>
            </div>
            <div class="form-group">
                <label asp-for="IdGrupoEjercicios" class="control-label"></label>
                <select asp-for="IdGrupoEjercicios" class ="form-control" asp-items="ViewBag.GrupoEjerciciosId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

